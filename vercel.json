{
    "version": 2,
    "name": "music-album",
    "builds": [
       { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)",
        "dest": "/app.js",
        "methods": ["GET","POST","HEAD","PUT","PATCH","DELETE"],
       "headers": {
        "Access-Control-Allow-Origin": "*"
    }}
    ]
 }